{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "anda",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
    "fs": {
      "all": true,
      "scope": ["**"],
      "readFile": true,
      "writeFile": true,
      "readDir": true,
      "copyFile": false,
      "createDir": false,
      "removeDir": false,
      "removeFile": false,
      "renameFile": false,
      "exists": false
    },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "anda",
        "width": 800,
        "height": 600
      }
    ]
  }
}
